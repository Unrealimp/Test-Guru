<tr align="center">
  <td><%= Category.where(id: test.category_id)[0].title %></td>
  <td><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to "#{t('.show')}", admin_test_path(test) %> </td>
  <td><%= link_to "#{t('.edit')}", edit_admin_test_path(test) %> </td>
  <td>
    <%= link_to "#{t('.delete')}",
                admin_test_path(test),
                method: :delete,
                data: { confirm: "#{t('.confirm_delete')}"} %>
  </td>
  <td><%= button_to "#{t('.start')}", start_test_path(test), class: 'btn btn-success' %></td> 
</tr>